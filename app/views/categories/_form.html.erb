<%= form_with model: category, url: categories_path, local:true do |f| %>
  <%= f.text_field :name, autocomplete: "name",placeholder: 'カテゴリ名を入力' %>
  <%= f.submit "カテゴリを登録",class:"btn btn-sm btn-outline-secondary m-3" %>
<% end %>
<%= link_to "カテゴリ一覧・追加", categories_path, class:"btn btn-sm btn-outline-secondary" %>