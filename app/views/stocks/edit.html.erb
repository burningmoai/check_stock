<div class="container pb-5">
  <div class="row">
    <h3>ストックの編集</h3>
  </div>
  <div class="row">
    <%= form_with model: @stock, local:true do |f| %>
      <%= f.number_field :amount, autocomplete: "amount" %>
      <%= f.submit "編集内容を登録",class:"btn btn-sm btn-outline-secondary" %>
    <% end %>
  </div>
</div>
